{
  "name": "@nicolawealth/rate_limit",
  "version": "0.0.3",
  "main": "./dist/index.umd.js",
  "module": "./dist/index.umd.js",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/types/index.d.ts",
  "source": "src/index.ts",
  "exports": {
    ".": {
      "require": "./dist/index.umd.js",
      "import": "./dist/index.umd.js"
    }
  },
  "files": [
    "dist/"
  ],
  "author": "Nicola Wealth Inc",
  "contributors": [
    {
      "name": "Cliff Hammerschmidt"
    },
    {
      "name": "Skye Haik"
    },
    {
      "name": "Travis Rempel"
    },
    {
      "name": "Mel Delosada"
    }
  ],
  "license": "MIT",
  "scripts": {
    "build": "microbundle --format umd && tsc --emitDeclarationOnly --declaration --outDir dist/types",
    "clean": "rm -rf dist/* && rm -f tsconfig.tsbuildinfo",
    "rename:esm": "/bin/zsh ./scripts/fix-mjs.sh",
    "lint": "eslint src",
    "test": "mocha",
    "test-r": "mocha -R spec",
    "cover": "nyc mocha",
    "cover:report": "nyc --reporter=html --reporter=lcov --reporter=text --reporter=json mocha -c --reporter=spec",
    "doc": "npx typedoc src/*.ts src/**/*.ts --out dist/doc",
    "check-audit": "check-audit"
  },
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "@nicolawealth/ioc": "^1.0.0"
  },
  "devDependencies": {
    "@nicolawealth/code_coverage_extractor": "^2",
    "@nicolawealth/resolved_promise": "^0.0.2",
    "@types/mocha": "^9",
    "@types/node": "^22",
    "typescript-eslint": "^8.48.1",
    "eslint": "^9",
    "microbundle": "^0.15.1",
    "mocha": "^10",
    "mocha-junit-reporter": "^2",
    "npm-audit-resolver": "^3.0.0-RC.0",
    "nyc": "^15",
    "ts-node": "^10",
    "typescript": "^5"
  },
  "packageManager": "npm@10.9.0"
}
